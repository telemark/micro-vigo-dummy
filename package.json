  },
  "bugs": {
    "url": "http://github.com/zrrrzzt/micro-vigo-dummy/issues"
  },
  "now": {
    "alias": "vigo.dummy.allthethings.win"
  },
  "devDependencies": {
    "coveralls": "2.11.16",
    "nsp": "2.6.3",
    "standard": "8.6.0",
    "tap": "10.2.0"
  },
  "dependencies": {
    "micro": "7.0.2"
